{
   "manifest_version": 2,
   "version": "2.3",
   "name": "SoundCloud Enhancer - Darkmode and more",
   "short_name": "SCE",
   "description": "This extension gives you tools, that will make your SoundCloud more customizable.",
   "homepage_url": "http://benjaminbachjensen.com",
   "icons": {
     "16": "assets/icons/icon-16.png",
     "48": "assets/icons/icon-48.png",
     "128": "assets/icons/icon-128.png"
   },
   "content_scripts": [ {
      "js": ["assets/js/jquery.min.js","assets/js/vex.combined.min.js", "assets/js/insignia.min.js", "assets/js/enhancer.js"],
      "css" : ["assets/css/vex.css", "assets/css/vex-theme-top.css", "assets/css/insignia.min.css", "assets/css/style.css"],
      "matches": [ "https://soundcloud.com/*" ],
      "run_at": "document_end"
   } ],
   "browser_action": {
      "default_icon": {
         "19": "assets/icons/icon-16.png",
         "38": "assets/icons/icon-38.png"
      },
      "default_title": "SoundCloud Enhancer",
      "default_popup": "assets/html/popup.html"
   },
   "permissions": [ "storage", "https://soundcloud.com/" ]
}
